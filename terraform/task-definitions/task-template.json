[
    {
        "name": "${name}",
        "image": "${image}",
        "essential": true,
        "logConfigurations": {
            "logDriver": "awslogs",
            "secretOptions": [],
            "options": {
                "awslogs-group": "/ecs/${name}-logs",
                "awslogs-region": "eu-west-1",
                "awslogs-stream-prefix": "ecs"
            }
        },
        "portMappings": [
            {
                "containerPort": 8080,
                "hostPort": 8080
            }
        ],
        "repositoryCredentials": {
            "credentialsParameter": "arn:aws:secretsmanager:eu-west-1:108782061116:secret:iamind_docker_hub_secret"
        }
    }
]